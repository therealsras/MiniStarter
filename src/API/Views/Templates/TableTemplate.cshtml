@model API.ViewModels.TableViewModel<DistrictDto>;

@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Table</title>
    <link href="https://fonts.googleapis.com/css2?family=Moul&family=Noto+Sans+Khmer&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <style>
        .khmer-moul {
            font-family: 'Moul', cursive;
        }
        .khmer-content {
            font-family: 'Noto Sans Khmer', sans-serif;
        }
    
    </style>
</head>

<body>
    <table class="table table-striped">
        <thead class="thead-success">
            <tr>
                <th scope="col">Type</th>
                <th scope="col">Code</th>
                <th scope="col">Khmer</th>
                <th scope="col">English</th>
            </tr>
        </thead>
        <tbody>
            @foreach(DistrictDto item in Model!.items)
            {
            <tr>
                <td>@item.Type</td>
                <td>@item.Code</td>
                <td>@item.NameKH</td>
                <td>@item.NameEN</td>
            </tr>
            }  
        </tbody>
    </table>
</body>
</html>